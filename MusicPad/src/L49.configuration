 if statusChannel 10
  if channelState SELECTED
   sendDeviceMessage 1 144 0 113 127
   return
  if channelState ACTIVE
   sendDeviceMessage 1 144 0 113 52
   return
  if channelState INACTIVE
   sendDeviceMessage 1 144 0 113 7
